{
  "manifest_version": 3,
  "name": "Chrome Tab Auto Grouping",
  "version": "1.0.0",
  "description": "Automatically group tabs by domain for better organization",
  "permissions": [
    "tabs",
    "tabGroups"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Chrome Tab Auto Grouping"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}